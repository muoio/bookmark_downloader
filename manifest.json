{
    "manifest_version": 3,
    "name": "Bookmark Downloader",
    "version": "1.0",
  
    "description": "Bookmark Downloader",
    "icons": {
        "48": "icon.png"
      },
    "action": {
        "default_icon": "icon.png",
        "default_title": "Bookmark Downloader",    
        "default_popup": "popup.html"
      },
    "background": {
      "service_worker": "js/background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "permissions":[
      "bookmarks"
    ]
  }
